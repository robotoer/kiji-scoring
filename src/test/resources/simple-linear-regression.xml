  <DataDictionary numberOfFields="2">
    <DataField name="predictor" optype="continuous" dataType="double"/>
    <DataField name="predicted" optype="continuous" dataType="double"/>
  </DataDictionary>
  <RegressionModel
      modelName="linearreg"
      functionName="regression"
      algorithmName="linearRegression"
      targetFieldName="predicted">
    <MiningSchema>
      <MiningField name="predictor"/>
      <MiningField name="predicted" usageType="predicted"/>
    </MiningSchema>
    <RegressionTable intercept="1">
      <NumericPredictor name="predictor" exponent="1" coefficient="2"/>
    </RegressionTable>
  </RegressionModel>
</PMML>